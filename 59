def uniquePaths(m: int, n: int) -> int:
    # Create a 2D array initialized to 1
    dp = [[1] * n for _ in range(m)]
    
    # Fill the dp array
    for i in range(1, m):
        for j in range(1, n):
            dp[i][j] = dp[i - 1][j] + dp[i][j - 1]
    
    return dp[m - 1][n - 1]

# Example usage
m = 3
n = 7
print(uniquePaths(m, n))  # Output: 28
